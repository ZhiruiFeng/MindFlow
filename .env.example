# MindFlow Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit the real .env file to version control!

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORG_ID=your_org_id_here_optional

# ElevenLabs API Configuration (Optional)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Development Settings
DEBUG_MODE=true
LOG_LEVEL=info

# API Endpoints (if using custom endpoints)
# OPENAI_API_BASE_URL=https://api.openai.com/v1
# ELEVENLABS_API_BASE_URL=https://api.elevenlabs.io/v1

# Note: In production, use Keychain Services instead of environment variables
# This file is for development convenience only
